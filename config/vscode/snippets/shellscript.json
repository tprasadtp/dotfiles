{
  "Shellscript Logging functions": {
    "prefix": "sh-loggers",
    "body": [
      "# Logging Handlers",
      "",
      "# Define colors for logging",
      "function define_colors()",
      "{",
      "\tdeclare -gr YELLOW=$'\\e[38;5;214m'",
      "\tdeclare -gr GREEN=$'\\e[38;5;83m'",
      "\tdeclare -gr RED=$'\\e[38;5;197m'",
      "\tdeclare -gr NC=$'\\e[0m'",
      "",
      "\t# Enhanced colors",
      "\tdeclare -gr PINK=$'\\e[38;5;212m'",
      "\tdeclare -gr BLUE=$'\\e[38;5;81m'",
      "\tdeclare -gr ORANGE=$'\\e[38;5;208m'",
      "\tdeclare -gr TEAL=$'\\e[38;5;192m'",
      "\tdeclare -gr VIOLET=$'\\e[38;5;219m'",
      "\tdeclare -gr GRAY=$'\\e[38;5;250m'",
      "\tdeclare -gr DARK_GRAY=$'\\e[38;5;246m'",
      "",
      "\t# Flag",
      "\tdeclare -gr COLORIZED=1",
      "}",
      "",
      "function undefine_colors()",
      "{",
      "\t# Disable all colors",
      "\tdeclare -gr YELLOW=\"\"",
      "\tdeclare -gr GREEN=\"\"",
      "\tdeclare -gr RED=\"\"",
      "\tdeclare -gr NC=\"\"",
      "",
      "\t# Enhanced colors",
      "\tdeclare -gr PINK=\"\"",
      "\tdeclare -gr BLUE=\"\"",
      "\tdeclare -gr ORANGE=\"\"",
      "\tdeclare -gr TEAL=\"\"",
      "\tdeclare -gr VIOLET=\"\"",
      "\tdeclare -gr GRAY=\"\"",
      "\tdeclare -gr DARK_GRAY=\"\"",
      "",
      "\t# Flag",
      "\tdeclare -gr COLORIZED=1",
      "}",
      "",
      "# Check for Colored output",
      "if [[ -n \\${CLICOLOR_FORCE} ]] && [[ \\${CLICOLOR_FORCE} != \"0\" ]]; then",
      "\t# In CI/CD Forces colors",
      "\tdefine_colors",
      "elif [[ -t 1 ]] && [[ -z \\${NO_COLOR} ]] && [[ \\${TERM} != \"dumb\" ]] ; then",
      "\t# Enables colors if Terminal is interactive and NOCOLOR is not empty",
      "\tdefine_colors",
      "else",
      "\t# Disables colors",
      "\tundefine_colors",
      "fi",
      "",
      "## Check if logs should be written to stderr",
      "## This is useful if script generates an output which can be piped or redirected",
      "if [[ -z \\${LOG_TO_STDERR} ]]; then",
      "\tLOG_TO_STDERR=\"false\"",
      "fi",
      "",
      "# Log functions",
      "function log_info()",
      "{",
      "\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\tprintf \"• %s \\n\" \"\\$@\" 1>&2",
      "\telse",
      "\t\tprintf \"• %s \\n\" \"\\$@\"",
      "\tfi",
      "}",
      "",
      "function log_success()",
      "{",
      "\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${GREEN}\" \"\\$@\" \"\\${NC}\" 1>&2",
      "\telse",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${GREEN}\" \"\\$@\" \"\\${NC}\"",
      "\tfi",
      "}",
      "",
      "function log_warning()",
      "{",
      "\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${YELLOW}\" \"\\$@\" \"\\${NC}\" 1>&2",
      "\telse",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${YELLOW}\" \"\\$@\" \"\\${NC}\"",
      "\tfi",
      "}",
      "",
      "function log_error()",
      "{",
      "\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${RED}\" \"\\$@\" \"\\${NC}\" 1>&2",
      "\telse",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${RED}\" \"\\$@\" \"\\${NC}\"",
      "\tfi",
      "}",
      "",
      "function log_debug()",
      "{",
      "\tif [[ LOG_LVL -gt 0  ]]; then",
      "\t\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\t\tprintf \"%s• %s %s\\n\" \"\\${GRAY}\" \"\\$@\" \"\\${NC}\" 1>&2",
      "\t\telse",
      "\t\t\tprintf \"%s• %s %s\\n\" \"\\${GRAY}\" \"\\$@\" \"\\${NC}\"",
      "\t\tfi",
      "\tfi",
      "}",
      "",
      "function log_notice()",
      "{",
      "\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${TEAL}\" \"\\$@\" \"\\${NC}\" 1>&2",
      "\telse",
      "\t\tprintf \"%s• %s %s\\n\" \"\\${TEAL}\" \"\\$@\" \"\\${NC}\"",
      "\tfi",
      "}",
      "",
      "function log_variable()",
      "{",
      "\tlocal var",
      "\tvar=\"\\$1\"",
      "\tif [[ \\${LOG_LVL} -gt 0  ]]; then",
      "\t\tif [[ \\$LOG_TO_STDERR == \"true\" ]]; then",
      "\t\t\tprintf \"%s» %-20s - %-10s %s\\n\" \"\\${GRAY}\" \"\\${var}\" \"\\${!var}\" \"\\${NC}\" 1>&2",
      "\t\telse",
      "\t\t\tprintf \"%s» %-20s - %-10s %s\\n\" \"\\${GRAY}\" \"\\${var}\" \"\\${!var}\" \"\\${NC}\"",
      "\t\tfi",
      "\tfi",
      "}"
    ],
    "description": "Shellscript Logging functions"
  },
  "Check if command is available": {
    "prefix": "sh-has-command",
    "body": [
      "# Checks if command is available",
      "function has_command() {",
      "\tif command -v \"\\$1\" >/dev/null; then",
      "\t\treturn 0",
      "\telse",
      "\t\treturn 1",
      "\tfi",
      "\treturn 1",
      "}",
      ""
    ],
    "description": "Check if command is available on the system"
  },
  "Check-Errored-Items": {
    "prefix": "sh-check-errored-items",
    "body": [
      "\tif [[ \\${#ERRORS[@]} -eq 0 ]]; then",
      "\t\tlog_success \"Success\"",
      "\telse",
      "\t\tlog_error \"Failed for : \\${ERRORS[*]}\"",
      "\t\texit 1",
      "\tfi",
      ""
    ],
    "description": "Checks if array ERRORS has more than one element",
    "scope": "shellscript"
  },
  "Semver RegEx Define": {
    "prefix": "sh-const-semver",
    "body": [
      "readonly SEMVER_REGEX=\"^[vV]?(0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)(\\-(0|[1-9][0-9]*|[0-9]*[A-Za-z-][0-9A-Za-z-]*)(\\.(0|[1-9][0-9]*|[0-9]*[A-Za-z-][0-9A-Za-z-]*))*)?(\\+[0-9A-Za-z-]+(\\.[0-9A-Za-z-]+)*)?\\$\"",
      ""
    ],
    "description": "Semver RegEx"
  },
