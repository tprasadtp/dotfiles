{
  "breadcrumbs.enabled": true,
  //CMake, ROS Helper stuff
  "cmake.configureOnOpen": false,
  "cSpell.userWords": [
    "Prasad",
    "Tengse",
    "autogen",
    "cache",
    "dependabot",
    "devtools",
    "direnv",
    "docgen",
    "tprasadtp",
    "venv"
  ],
  "dart.allowAnalytics": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.fontFamily": "'StarshipCode', 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": true,
  "explorer.confirmDelete": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "files.associations": {
    ".envrc": "shellscript",
    "Dockerfile.*": "dockerfile",
    "poetry.*": "toml"
  },
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.pytest_cache/**": true,
    "**/.tox/**": true,
    "**/.venv/**": true,
    "**/node_modules/*/**": true,
    "**/venv/**": true
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  //Go Tools and Settings
  "go.formatTool": "goimports",
  //Json,NPM Disable downloads
  "json.schemaDownload.enable": false,
  "npm.fetchOnlinePackageInfo": false,
  //Python Stuff
  "python.analysis.logLevel": "Warning",
  "python.autoUpdateLanguageServer": false,
  "python.dataScience.sendSelectionToInteractiveWindow": false,
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "python.languageServer": "Microsoft",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": ".venv/bin/python",
  "python.showStartPage": false,
  "search.exclude": {
    "**/.git": true,
    "**/.venv": true
  },
  "settingsSync.keybindingsPerPlatform": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "StarshipMono",
  "terminal.integrated.fontSize": 16,
  "terraform.indexing.enabled": false,
  "terraform.telemetry.enabled": false,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": [],
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",
  //YAML, This is required for Cloudformation Tags
  "yaml.customTags": [
    "!fn",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join sequence",
    "!vault"
  ],
  //AWS SDK
  "aws.cdk.explorer.enabled": false,
  "aws.telemetry": false,
  // CodeSpaces
  "codespaces.accountProvider": "GitHub",
  //Icons and Themes
  "workbench.iconTheme": "material-icon-theme",
  //Icon Theme
  "material-icon-theme.saturation": 1,
  "material-icon-theme.showWelcomeMessage": false,
  //Themes
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
  "workbench.preferredLightColorTheme": "Gruvbox Dark Soft"
}
