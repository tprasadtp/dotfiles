{
  "breadcrumbs.enabled": true,
  //CMake, ROS Helper stuff
  "cmake.configureOnOpen": false,
  "cSpell.userWords": [
    "Prasad",
    "Tengse",
    "cache",
    "dependabot",
    "tprasadtp",
    "venv"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": true,
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": true,
  "explorer.confirmDelete": false,
  //Extensions disable auto update
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "files.associations": {
    ".envrc": "shellscript",
    "Dockerfile.*": "dockerfile",
    "poetry.*": "toml",
    "*.hcl": "hcl"
  },
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.pytest_cache/**": true,
    "**/.tox/**": true,
    "**/.venv/**": true,
    "**/node_modules/*/**": true,
    "**/venv/**": true
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  //Go Tools and Settings
  "go.formatTool": "goimports",
  //Json,NPM Disable downloads
  "json.schemaDownload.enable": false,
  "npm.fetchOnlinePackageInfo": false,
  //Python Stuff
  "python.analysis.logLevel": "Warning",
  "python.autoUpdateLanguageServer": false,
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": ".venv/bin/python",
  "python.showStartPage": false,
  "jupyter.sendSelectionToInteractiveWindow": false,
  "search.exclude": {
    "**/.git": true,
    "**/.venv": true
  },
  "settingsSync.keybindingsPerPlatform": false,
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": [],
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",
  //YAML, This is required for Cloudformation Tags
  "yaml.customTags": [
    "!fn",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join sequence",
    "!vault"
  ],
  //InterlliCode disable not needed stuff
  "vsintellicode.java.completionsEnabled": false,
  "vsintellicode.sql.completionsEnabled": false,
  // CodeSpaces
  "codespaces.accountProvider": "GitHub",
  "codespaces.featureSet": "stable",
  "codespaces.enableSubmoduleCloning": true,
  //Disable Telemetry for various stuff
  //------------------------------------------------
  //core
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  //dart
  "dart.allowAnalytics": false,
  //AWS
  "aws.telemetry": false,
  "aws.cdk.explorer.enabled": false,
  //------------------------------------------------
  //Icons and Themes
  //------------------------------------------------
  "workbench.iconTheme": "material-icon-theme",
  //Icon Theme
  "material-icon-theme.saturation": 1,
  "material-icon-theme.showWelcomeMessage": false,
  "material-icon-theme.activeIconPack": "vue_vuex",
  //Themes
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
  "workbench.preferredLightColorTheme": "Gruvbox Light Hard",
  "material-icon-theme.files.associations": {
    "*.ts": "typescript",
    "**/ansible/*.yml": "ansible",  },
  "editor.semanticHighlighting.enabled": true,
  "debug.console.fontSize": 18,
  "terminal.integrated.fontFamily": "Firacode Nerd font",
  "terminal.integrated.fontWeight": 400,
  "go.useLanguageServer": true,
  "go.addTags": {
    "tags": "json,yaml",
    "options": "",
    "transform": "camelcase"
  },
  "material-icon-theme.folders.associations": {
    "generate.*.*": "generator",
    "generate": "generator",
    "gen.*.*": "generator",
    "inventory": "server",
    "templates": "template",
    "roles": "ansible",
    "vault": "keys",
    ".venv": "python",
    "venv": "python"
  },
  //Remote
  "remote.SSH.enableAgentForwarding": false,
  "dotfiles.repository": "https://github.com/tprasadtp/dotfiles",
  "dotfiles.installCommand": "~/dotfiles/install.sh --codespaces",
  //Disable GitHub auth
  "github.gitAuthentication": false
}
